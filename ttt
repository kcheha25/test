max_pics = max(len(p) for p in y_heights)  # Trouver le max de pics dans un chromatogramme

y_heights = pad_sequences(y_heights, maxlen=max_pics, dtype="float32", padding="post")
y_bounds = pad_sequences(y_bounds, maxlen=max_pics, dtype="float32", padding="post")
y_components = pad_sequences(y_components, maxlen=max_pics, dtype="int32", padding="post")